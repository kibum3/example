<mapper namespace="com.project.mapper.BoardMapper">

	<!-- 게시글 등록 처리 -->
	<insert id="create">
		insert into board
		(
		board_no,
		title,
		content,
		writer
		)
		values
		(
		board_seq.nextval,
		#{title},
		#{content},
		#{writer}
		)
	</insert>
</mapper>